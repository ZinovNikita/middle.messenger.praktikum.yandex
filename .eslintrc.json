{
    "extends":[
        "eslint:recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "parseOptions":{
        "ecmaVersion": 6
    },
    "rules": {
        "no-unused-vars": 2,
        "@typescript-eslint/no-unused-vars": 2,
        "max-len": [1, 100],
        "max-params": [2, 3]
    }
}